#: controller.py:73
msgid "HTML Headers"
msgstr "Intestazioni HTML"

#: plugin.py:31
msgid ""
"Plugin adds wiki-commands (:title:), (:description:), (:keywords:) and (:"
"htmlhead:)"
msgstr ""
"Il plugin aggiunge i comandi wiki (:title:), (:description:), (:keywords:) e "
"(:htmlhead:)"

#: plugin.py:33
msgid ""
"<b>Usage:</b>\n"
"(:title Page title:)\n"
"\n"
"(:description Page description:)\n"
"\n"
"(:keywords keyword_1, keyword_2, other keyword:)\n"
"\n"
"(:htmlhead:)\n"
"&lt;meta http-equiv='Content-Type' content='text/html; charset=utf-8' /&gt;\n"
"\n"
"&lt;meta name='robots' content='index,follow' /&gt;\n"
"(:htmlheadend:)\n"
msgstr ""
"<b>Utilizzo:</b>\n"
"(:title Titolo pagina:)\n"
"\n"
"(:description Descrizione pagina:)\n"
"\n"
"(:keywords ParolaChiave_1, ParolaChiave_2, altreParoleChiave:)\n"
"\n"
"(:htmlhead:)\n"
"&lt;meta http-equiv='Content-Type' content='text/html; charset=utf-8' /&gt;\n"
"\n"
"&lt;meta name='robots' content='index,follow' /&gt;\n"
"(:htmlheadend:)\n"

