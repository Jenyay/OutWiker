#: plugin.py:49
msgid ""
"<B>Usage:</B>:\n"
"(:source params... :)\n"
"source code\n"
"(:sourceend:)"
msgstr ""
"<B>Usage:</B>:\n"
"(:source params... :)\n"
"source code\n"
"(:sourceend:)"

#: plugin.py:54
msgid ""
"<B>Params:</B>\n"
"<U>lang</U> - programming language\n"
"\n"
"<U>tabwidth</U> - tab size\n"
"\n"
"<U>file</U> - attached source file\n"
"\n"
"<U>encoding</U> - encoding of the attached source file (default encoding - "
"utf8)\n"
"\n"
"<U>style</U> - style of hightlighting\n"
"\n"
"<U>parentbg</U> - use the page background for the code block\n"
"\n"
"<U>linenum</U> - enable line numbers"
msgstr ""
"<B>Params:</B>\n"
"<U>lang</U> - programming language\n"
"\n"
"<U>tabwidth</U> - tab size\n"
"\n"
"<U>file</U> - attached source file\n"
"\n"
"<U>encoding</U> - encoding of the attached source file (default encoding - "
"utf8)\n"
"\n"
"<U>style</U> - style of hightlighting\n"
"\n"
"<U>parentbg</U> - use the page background for the code block\n"
"\n"
"<U>linenum</U> - enable line numbers"

#: plugin.py:69
msgid ""
"<B>Example 1:</B>\n"
"<PRE>(:source lang=\"python\" tabwidth=4:)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
"(:sourceend:)\n"
"</PRE>"
msgstr ""
"<B>Example 1:</B>\n"
"<PRE>(:source lang=\"python\" tabwidth=4:)\n"
"import os\n"
"\n"
"if __name__ == \"__main__\":\n"
"    print \"Hello World!\"\n"
"(:sourceend:)\n"
"</PRE>"

